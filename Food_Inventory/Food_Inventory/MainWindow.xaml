        <!--#region CategoriesPanel-->
        <Grid x:Name="CategoriesPanel_Grid" Grid.Column="2" Grid.Row="2" Visibility="Hidden">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="17*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="11*" MaxWidth="375"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>

            <Grid x:Name="CategoriesPanel_List_Grid" Grid.Column="0" Grid.Row="0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="2*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="36*"/>
                </Grid.RowDefinitions>

                <Grid x:Name="CategoriesPanel_List_Search_Grid" Grid.Column="0" Grid.Row="1">
                    <StackPanel x:Name="CategoriesPanel_List_Search_Stack_Panel" Orientation="Horizontal" HorizontalAlignment="Right">
                        <Button x:Name="CategoriesPanel_List_Search_Button" BorderThickness="0,0,0,0" Width="32" Height="32" Click="CategoriesPanel_List_Search_Button_Click">
                            <Button.Background>
                                <ImageBrush ImageSource="/Search.png"/>
                            </Button.Background>
                        </Button>

                        <Grid x:Name="CategoriesPanel_List_Search_Field_Grid" Visibility="Collapsed">
                            <Label x:Name="CategoriesPanel_List_Search_Field_Label" Content="Поиск" Padding="20,0,0,0" UseLayoutRounding="False" Visibility="Visible" FontSize="14" VerticalContentAlignment="Center" Foreground="#FFA7AEBD" Background="#FFFAFAFB"/>
                            <TextBox x:Name="CategoriesPanel_List_Search_Field_TextBox" Visibility="Visible" TextWrapping="NoWrap" BorderBrush="#FF1C1D1F" MinWidth="120" MinHeight="28" Margin="14,0,0,0" VerticalAlignment="Center" VerticalContentAlignment="Center" FontSize="14" BorderThickness="1,1,1,1" Background="{x:Null}" Width="120" Height="26" Padding="5,0,0,0" TextChanged="CategoriesPanel_List_Search_Field_TextBox_TextChanged" MaxLength="35"/>
                        </Grid>
                    </StackPanel>
                </Grid>

                <Grid x:Name="CategoriesPanel_List_Window_Grid" Grid.Column="0" Grid.Row="4">
                    <ScrollViewer x:Name="CategoriesPanel_List_Window_ScrollViewer">
                        <StackPanel>
                            <StackPanel Margin="0,0,0,5">
                                <Border Height="35" Background="#FFA7AEBD" VerticalAlignment="Top" Margin="0,0,0,5">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="8*"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>

                                        <Label Grid.Column="0" Grid.Row="0" Content="БЛЮДА" Foreground="#FF090B0F" HorizontalContentAlignment="Left" Padding="15,1,1,1" FontSize="20" VerticalAlignment="Center"/>
                                        <Button Grid.Column="1" Grid.Row="0" BorderThickness="0,0,0,0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Margin="0,0,15,0">
                                            <Button.Background>
                                                <ImageBrush ImageSource="/Minus.png" Stretch="None"/>
                                            </Button.Background>
                                        </Button>
                                    </Grid>
                                </Border>

                                <StackPanel>
                                    <StackPanel Orientation="Horizontal">
                                        <Button Margin="15,0,5,0" Height="20" Width="20">
                                            <Button.Background>
                                                <ImageBrush ImageSource="/Delete.png" Stretch="None"/>
                                            </Button.Background>
                                        </Button>
                                        <Label Content="Гарниры" FontSize="18" Padding="1,1,1,1"/>
                                    </StackPanel>
                                </StackPanel>
                            </StackPanel>
                        </StackPanel>
                    </ScrollViewer>
                    <Border BorderBrush="#FF090B0F" BorderThickness="2"/>


                </Grid>

            </Grid>

            <Grid x:Name="CategoriesPanel_AddCategory_Grid" Grid.Column="2" Grid.Row="0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="3*"/>
                    <RowDefinition Height="4*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="4*"/>
                    <RowDefinition Height="3*"/>
                </Grid.RowDefinitions>

                <Grid x:Name="CategoriesPanel_AddCategory_Dish_Grid" Grid.Column="0" Grid.Row="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="6*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="2*"/>
                    </Grid.RowDefinitions>

                    <Grid x:Name="CategoriesPanel_AddCategory_Dish_Window_Grid" Grid.Column="0" Grid.Row="0" Background="#FFA7AEBD">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*" MaxHeight="45"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="5*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>

                        <Grid x:Name="CategoriesPanel_AddCategory_Dish_Field_Grid" Grid.Column="1" Grid.Row="1">
                            <Canvas x:Name="CategoriesPanel_AddCategory_Dish_Field_Canvas" Background="#FFFAFAFB"/>
                            <Label x:Name="CategoriesPanel_AddCategory_Dish_Field_Label" Content="Категория блюда" Padding="10,0,0,0" UseLayoutRounding="False" Visibility="Visible" FontSize="20" VerticalContentAlignment="Center" Foreground="#FFA7AEBD" Background="#FFFAFAFB"/>
                            <TextBox x:Name="CategoriesPanel_AddCategory_Dish_Field_TextBox" VerticalContentAlignment="Center" FontSize="20" Foreground="#FF090B0F" Padding="10,0,0,0" SelectionBrush="#FFA7AEBD" TextChanged="CategoriesPanel_AddCategory_Dish_Field_TextBox_TextChanged" Background="{x:Null}" BorderThickness="0,0,0,0" MaxLength="35"/>
                        </Grid>
                    </Grid>

                    <Grid x:Name="CategoriesPanel_AddCategory_Dish_Button_Grid" Grid.Column="0" Grid.Row="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="2*"/>
                            <ColumnDefinition Width="3*"/>
                            <ColumnDefinition Width="2*"/>
                        </Grid.ColumnDefinitions>

                        <Button x:Name="CategoriesPanel_AddCategory_Dish_Button" Grid.Column="1" Grid.Row="0" Content="ДОБАВИТЬ" Background="#FFA7AEBD" BorderThickness="0,0,0,0" FontSize="14" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Padding="0,0,0,0" MaxWidth="160" MaxHeight="50"/>
                    </Grid>
                </Grid>

                <Grid x:Name="CategoriesPanel_AddCategory_Diet_Grid" Grid.Column="0" Grid.Row="3">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="6*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="2*"/>
                    </Grid.RowDefinitions>

                    <Grid x:Name="CategoriesPanel_AddCategory_Diet_Window_Grid" Grid.Column="0" Grid.Row="0" Background="#FFA7AEBD">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*" MaxHeight="45"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="5*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>

                        <Grid x:Name="CategoriesPanel_AddCategory_Diet_Field_Grid" Grid.Column="1" Grid.Row="1">
                            <Canvas x:Name="CategoriesPanel_AddCategory_Diet_Field_Canvas" Background="#FFFAFAFB"/>
                            <Label x:Name="CategoriesPanel_AddCategory_Diet_Field_Label" Content="Категория диеты" Padding="10,0,0,0" UseLayoutRounding="False" Visibility="Visible" FontSize="20" VerticalContentAlignment="Center" Foreground="#FFA7AEBD" Background="#FFFAFAFB"/>
                            <TextBox x:Name="CategoriesPanel_AddCategory_Diet_Field_TextBox" VerticalContentAlignment="Center" FontSize="20" Foreground="#FF090B0F" Padding="10,0,0,0" SelectionBrush="#FFA7AEBD" TextChanged="CategoriesPanel_AddCategory_Diet_Field_TextBox_TextChanged" Background="{x:Null}" BorderThickness="0,0,0,0" MaxLength="35"/>
                        </Grid>
                    </Grid>

                    <Grid x:Name="CategoriesPanel_AddCategory_Diet_Button_Grid" Grid.Column="0" Grid.Row="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="2*"/>
                            <ColumnDefinition Width="3*"/>
                            <ColumnDefinition Width="2*"/>
                        </Grid.ColumnDefinitions>

                        <Button x:Name="CategoriesPanel_AddCategory_Diet_Button" Grid.Column="1" Grid.Row="0" Content="ДОБАВИТЬ" Background="#FFA7AEBD" BorderThickness="0,0,0,0" FontSize="14" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Padding="0,0,0,0" MaxWidth="160" MaxHeight="50"/>
                    </Grid>
                </Grid>
            </Grid>
        </Grid>
        <!--#endregion-->

        <Grid x:Name="ProductsPanel_Grid" Grid.Column="2" Grid.Row="2" Visibility="Hidden"></Grid>
        <Grid x:Name="DietPanel_Grid" Grid.Column="2" Grid.Row="2" Visibility="Hidden"></Grid>
        <Grid x:Name="LimitsPanel_Grid" Grid.Column="2" Grid.Row="2" Visibility="Hidden"></Grid>
        <Grid x:Name="StatisticsPanel_Grid" Grid.Column="2" Grid.Row="2" Visibility="Hidden"></Grid>
    </Grid>
</Window>
